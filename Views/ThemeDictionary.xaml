<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d">
	<DataTemplate x:Key="NewsFeedsListBoxItemTemplate">
		<Grid Margin="0,0,0,15">
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto"/>
				<RowDefinition Height="Auto"/>
			</Grid.RowDefinitions>
			<TextBlock Margin="0,0,1,0" TextWrapping="Wrap" Text="{Binding Title}" d:LayoutOverrides="Width, Height" FontSize="{StaticResource PhoneFontSizeMedium}"/>
			<TextBlock TextWrapping="Wrap" Text="{Binding RefreshTimeStamp.Value, Mode=OneWay}" Margin="0,0,1,0" VerticalAlignment="Top" Grid.Row="1"/>
		</Grid>
	</DataTemplate>
	<DataTemplate x:Key="FeedListBoxItemTemplate">
		<Grid>
			<TextBlock Margin="0,0,1,0" TextWrapping="Wrap" Text="{Binding Title}" d:LayoutOverrides="Width, Height" FontSize="{StaticResource PhoneFontSizeMedium}"/>
		</Grid>
	</DataTemplate>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>